        <mxCell id="eq9ACowOTim6wulsKato-18" value="BeanFactory和FactoryBean的区别" style="rounded=1;whiteSpace=wrap;html=1;fillColor=#d5e8d4;strokeColor=#82b366;fontStyle=1" parent="1" vertex="1">
